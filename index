<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>My Search</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      display: flex;
      justify-content: center;
      align-items: center;
      height: 100vh;
      background: white;
    }

    .box {
      text-align: center;
    }

    input {
      width: 300px;
      padding: 10px;
      font-size: 16px;
      border-radius: 20px;
      border: 1px solid #ccc;
    }

    button {
      margin-top: 15px;
      padding: 8px 20px;
      font-size: 14px;
      cursor: pointer;
    }
  </style>
</head>
<body>
  <div class="box">
    <h1>My Search</h1>
    <input id="q" placeholder="Search something...">
    <br>
    <button onclick="search()">Search</button>
  </div>

  <script>
    function search() {
      let query = document.getElementById("q").value;
      window.location.href =
        "https://www.google.com/search?q=" + encodeURIComponent(query);
    }
  </script>
</body>
</html>
